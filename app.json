{
  "expo": {
    "name": "sokumeishi",
    "slug": "sokumeishi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/icons/social/app-icon.png",
    "scheme": "sokumeishi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourdomain.sokumeishi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "assets/images/icons/social/app-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.yourdomain.sokumeishi"
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "assets/images/templates/vertical/1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "dynamicLinks": {
        "domain": "https://sokumeshi-58f06.web.app"
      },
      "router": {},
      "eas": {
        "projectId": "6db53103-8041-42b0-bb67-903a450e3112"
      }
    },
    "experiments": {
      "typedRoutes": false
    }
  }
}
